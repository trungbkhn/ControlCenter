<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/glV50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/glH50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />

        <com.tapbi.spark.controlcenter.feature.mishade.view.ItemCollapseRecyclerviewShade
            android:id="@+id/itemWifi"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@id/glH50"
            app:layout_constraintDimensionRatio="56:91"
            app:layout_constraintEnd_toEndOf="@id/glV50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


         <com.tapbi.spark.controlcenter.feature.mishade.view.ItemCollapseRecyclerviewShade
            android:id="@+id/itemDataMobi"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginHorizontal="10dp"
            app:layout_constraintBottom_toBottomOf="@id/glH50"
            app:layout_constraintDimensionRatio="56:91"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/glV50"
            app:layout_constraintTop_toTopOf="parent" />


        <com.tapbi.spark.controlcenter.feature.mishade.view.ItemCollapseRecyclerviewShade
            android:id="@+id/itemBluetooth"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginHorizontal="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="56:91"
            app:layout_constraintEnd_toEndOf="@id/glV50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/glH50" />


        <com.tapbi.spark.controlcenter.feature.mishade.view.ItemCollapseRecyclerviewShade
            android:id="@+id/itemAirplane"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginHorizontal="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="56:91"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/glV50"
            app:layout_constraintTop_toTopOf="@id/glH50" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>