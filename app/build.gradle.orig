apply plugin: 'com.google.gms.google-services'
apply plugin: 'io.fabric'

android {
    compileSdkVersion 26
    buildToolsVersion "26.0.2"
    defaultConfig {
        applicationId "com.tapbi.spark.controlcenter"
        minSdkVersion 19
        targetSdkVersion 26
        versionCode 1
        versionName "1.0"
